# Git
.git
.gitignore

# Development files
__dev__
*.log
*.DS_Store

# Test files
*.test.ts
*.test.js
*.spec.ts
*.spec.js
__tests__

# Documentation
*.md
docs/
reports/

# Scripts and configs
scripts/
.env.local
.env.development
.env.test

# Backup files
backups/
*.backup
*.save
*.broken

# SPA API (not needed for main deployment)
spa-api/

# Data files
data/exports/

# Temporary files
temp/
tmp/
*.tmp

# IDE files
.vscode/
.idea/

